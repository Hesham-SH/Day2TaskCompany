@{
}

@model List<Project>

@foreach(var item in Model)
{
    <h1>@item.Name</h1>
    <h1>@item.Number</h1>
    <h1>@item.Location</h1>
    <h1>@item.department</h1>
}
<h1>@ViewBag.entry</h1>
<button type="button"><a href="/project/AssignProjects/@ViewBag.id">Edit Projects</a></button>

